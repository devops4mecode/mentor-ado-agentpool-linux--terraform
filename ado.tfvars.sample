# Create Resource Group
rg_name = "do4m-ado-rg"

# Create Virtual Network
vnet_name                     = "do4m-ado-agent-vnet"
vnet_ip_address               = ["192.168.0.0/16"]
subnet_name_ado_agent         = "ado-agent-subnet"
subnet_name_ado_agent_address = "192.168.0.0/24"

# Key Vault
key_vault_name = "do4m-devops-kv"

# Create Virtual Machine
vm_name               = "do4m-ado"
vm_private_ip_address = "192.168.0.4"
vm_pip_name           = "do4m-ado-agent-pip"
vm_username           = "do4m-admin"
vm_osdisk_name        = "do4m-ado-vm-osdisk"

# Network Security Group
nsg_name = "do4m-ado-vm-nsg"

scfile="ado-agent.sh"